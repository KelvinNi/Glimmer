@prefix :        <http://www.w3.org/2002/07/owl#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ns:      <http://woo.corp.yahoo.com/ns/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ns:MusicGroup
      a       owl:Class ;
      rdfs:comment "A music group is a group of individuals who band together for the purpose of playing or creating music."@en ;
      owl:label "MusicGroup"@en ;
      owl:subClassOf
              [ a       owl:Class ;
                :intersectionOf (ns:Group ns:MusicArtist)
              ] .

ns:Ceremony
      a       owl:Class ;
      rdfs:comment "An event created to mark a special occasion or to give out awards. Examples: Oscars, Emmys, Tonys, Nobel. Not to be confused with the actual awards themselves."@en ;
      rdfs:label "Ceremony"@en ;
      owl:subClassOf ns:Event .

ns:connectionTo
      a       owl:ObjectProperty ;
      rdfs:comment "connectionTo connects an organization to the reified OrgAffiliation   (need to represent an entity's affiliation to org for time x)"@en ;
      rdfs:domain ns:Organization ;
      rdfs:label "connectionTo"@en ;
      rdfs:range ns:OrgAffiliation .

ns:url
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Specific URL for a webpage."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:EventVenue ns:Image ns:LocalEvent ns:OnlineAccount ns:Webpage)
              ] ;
      rdfs:label "url"@en ;
      rdfs:range xsd:anyURI ;
      owl:subPropertyOf :topDataProperty .

ns:publisher
      a       :FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "Connects a Media Work to its publisher."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:MediaWork ns:Synopsis)
              ] ;
      rdfs:label "publisher"@en ;
      rdfs:range ns:Publisher ;
      :inverseOf ns:publisherOf .

ns:parking
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the type and availability of parking at a location. Allowed values: has_parking, free, paid, valet, no_parking "@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:LocalBusiness ns:Structure)
              ] ;
      rdfs:label "parking"@en ;
      rdfs:range owl:Literal ;
      rdfs:range
              [ a       owl:Datatype ;
                :oneOf  [ a       rdf:List ;
                          rdf:first "FREE"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "HAS PARKING"^^xsd:string ;
                                    rdf:rest
                                            [ a       rdf:List ;
                                              rdf:first "NO PARKING"^^xsd:string ;
                                              rdf:rest
                                                      [ a       rdf:List ;
                                                        rdf:first "PAID"^^xsd:string ;
                                                        rdf:rest
                                                                [ a       rdf:List ;
                                                                  rdf:first "STREET"^^xsd:string ;
                                                                  rdf:rest
                                                                          [ a       rdf:List ;
                                                                            rdf:first "VALET"^^xsd:string ;
                                                                            rdf:rest ()
                                                                          ]
                                                                ]
                                                      ]
                                            ]
                                  ]
                        ]
              ] ;
      owl:subPropertyOf :topDataProperty .

ns:featureRating
      a       owl:DatatypeProperty ;
      rdfs:comment "Rating assigned to the special features available on a DVD."@en ;
      rdfs:domain ns:Video ;
      rdfs:label "featureRating"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf :topDataProperty .

ns:financeInstrumentName
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "financenstrumentName is the name (if any) for this quotable instrument. For example, Yahoo! Common Stock on NASDAQ. "@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:FinancialInstrument ns:QuotableInstrument)
              ] ;
      rdfs:label "financeInstrumentName"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf ns:name .

ns:stockExchange
      a       owl:ObjectProperty ;
      rdfs:comment "stockExchange connects a publicly traded company to its primary stock exchange"@en ;
      rdfs:domain ns:PublicCompany ;
      rdfs:label "stockExchange"@en ;
      rdfs:range ns:StockExchange ;
      :inverseOf ns:stockExchangeOf .

ns:SportGame
      a       owl:Class ;
      rdfs:comment "A sports game is a competitive event between individuals or teams."@en ;
      rdfs:label "SportGame"@en ;
      owl:subClassOf ns:LocalEvent .

ns:alcoholAllowed
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Indicates if alcohol is available at a local business."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:LocalBusiness ns:LocalEvent)
              ] ;
      rdfs:label "alcoholAllowed"@en ;
      rdfs:range xsd:boolean .

ns:deathDate
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "The date on which an individual becomes deceased."@en ;
      rdfs:domain ns:Person ;
      rdfs:label "deathDate"@en ;
      rdfs:range
              [ a       owl:Datatype ;
                :unionOf (xsd:date xsd:gYearMonth)
              ] ;
      owl:subPropertyOf ns:date .

ns:Director
      a       owl:Class ;
      rdfs:comment "A director is a person who directs the making or production of a film, television program, or play."@en ;
      rdfs:label "Director"@en ;
      owl:subClassOf ns:Person .

ns:healthcareFacilities
      a       owl:DatatypeProperty ;
      rdfs:comment "Enumerates the services offered by a hospital or other healthcare facility."@en ;
      rdfs:domain ns:Healthcare ;
      rdfs:label "healthcareFacilities"@en ;
      rdfs:range
              [ a       owl:Datatype ;
                :oneOf  [ a       rdf:List ;
                          rdf:first "AMBULANCE"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "BLOOD BANK"^^xsd:string ;
                                    rdf:rest
                                            [ a       rdf:List ;
                                              rdf:first "CASUALTY"^^xsd:string ;
                                              rdf:rest
                                                      [ a       rdf:List ;
                                                        rdf:first "DIAGNOSTIC SERVICES"^^xsd:string ;
                                                        rdf:rest
                                                                [ a       rdf:List ;
                                                                  rdf:first "ICU"^^xsd:string ;
                                                                  rdf:rest
                                                                          [ a       rdf:List ;
                                                                            rdf:first "MEDICAL SHOP"^^xsd:string ;
                                                                            rdf:rest ()
                                                                          ]
                                                                ]
                                                      ]
                                            ]
                                  ]
                        ]
              ] ;
      owl:subPropertyOf :topDataProperty .

ns:productionStatus
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Enumerates the status of a media work while in production."@en ;
      rdfs:domain ns:MediaWork ;
      rdfs:label "productionStatus"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf :topDataProperty .

ns:providerName
      a       owl:DatatypeProperty ;
      rdfs:comment "Name of the data provider"@en ;
      rdfs:domain ns:LocalBusiness ;
      rdfs:label "providerName"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf ns:name .

ns:nominated
      a       owl:ObjectProperty ;
      rdfs:comment "Connects a person, organization, or media work to the award nomination class."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:MediaWork ns:Organization ns:Person)
              ] ;
      rdfs:label "nominated"@en ;
      rdfs:range ns:Nomination ;
      owl:subPropertyOf :topObjectProperty .

ns:uriErrorCode
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Error code for the URI"@en ;
      rdfs:domain ns:Webpage ;
      rdfs:label "uriErrorCode"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf :topDataProperty .

ns:mediaMenu
      a       owl:DatatypeProperty ;
      rdfs:comment "Menu elements of a media product."@en ;
      rdfs:domain ns:MediaProduct ;
      rdfs:label "mediaMenu"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf :topDataProperty .

ns:contactNameLast
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Last name of the contact person for a local business (for use with SingleLocationBusiness only)."@en ;
      rdfs:domain ns:SingleLocationBusiness ;
      rdfs:label "contactNameLast"@en ;
      rdfs:range owl:Literal .

ns:timeZone
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "timeZone is a region that has a uniform, legally mandated standard time"@en ;
      rdfs:domain ns:Address ;
      rdfs:label "timezone"@en ;
      rdfs:range owl:Literal .

ns:flag
      a       owl:DatatypeProperty ;
      rdfs:comment "One-character code(s) providing additional descriptive information for a DVD."@en ;
      rdfs:domain ns:Video ;
      rdfs:label "flag"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf :topDataProperty .

ns:compensationOf
      a       :FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "compensationOf connects the type of compensation the the financial person who receives it."@en ;
      rdfs:domain ns:Compensation ;
      rdfs:label "compensationOf"@en ;
      rdfs:range ns:FinancePerson .

ns:personalRelationship
      a       owl:ObjectProperty ;
      rdfs:comment "Connects a person to the reified PersonalRelationship class."@en ;
      rdfs:domain ns:Person ;
      rdfs:label "personalRelationship"@en ;
      rdfs:range ns:PersonalRelationship ;
      owl:subPropertyOf :topObjectProperty .

ns:rates
      a       :FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "Connects a rating system to the films it categorizes according to thematic and content suitability for certain audiences."@en ;
      rdfs:domain ns:MovieRating ;
      rdfs:label "rates"@en ;
      rdfs:range
              [ a       owl:Class ;
                :unionOf (ns:Movie ns:Video)
              ] ;
      owl:subPropertyOf :topObjectProperty ;
      :inverseOf ns:rated .

ns:affiliation
      a       owl:ObjectProperty ;
      rdfs:comment "affiliation connects the reified class OrgAffiliation to a person, organization or shareholder"@en ;
      rdfs:domain ns:OrgAffiliation ;
      rdfs:label "affiliation"@en ;
      rdfs:range
              [ a       owl:Class ;
                :unionOf (ns:Organization ns:Person)
              ] ;
      :inverseOf ns:affiliationWith .

ns:soldOut
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Boolean attribute indicating if an event is sold out."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:Deal ns:EventRegistration ns:LocalEventOccurrence)
              ] ;
      rdfs:label "soldOut"@en ;
      rdfs:range xsd:boolean .

ns:TQIAffiliation
      a       owl:Class ;
      rdfs:comment "TQIAffiliation is a reified class linking a tradeablequoteableinstrument to a ticker"@en ;
      rdfs:label "TQIAffiliation"@en ;
      owl:subClassOf ns:Event .

ns:dealShortTitle
      a       owl:DatatypeProperty ;
      rdfs:comment "Abbreviated title of a deal, coupon, or offer."@en ;
      rdfs:domain ns:Deal ;
      rdfs:label "dealShortTitle"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf ns:label .

ns:mtLists
      a       owl:ObjectProperty ;
      rdfs:comment "mtLists connects a market tier to the tradablequotable instruments that it lists"@en ;
      rdfs:domain ns:MarketTier ;
      rdfs:label "mtLists"@en ;
      rdfs:range ns:TradableQuotableInstrument ;
      :inverseOf ns:mtListedOn .

ns:openingDays
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Number of days in a given week or weekend that a film was open."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:BoxOfficeWeekend ns:BoxOfficeWeekly)
              ] ;
      rdfs:label "openingDays"@en ;
      rdfs:range xsd:int ;
      owl:subPropertyOf :topDataProperty .

ns:valueText
      a       owl:DatatypeProperty ;
      rdfs:comment "example: 99.00USD"@en , "Indicates the actual value of a deal or offer."@en ;
      rdfs:label "valueText"@en .

ns:Island
      a       owl:Class ;
      rdfs:comment "placetype code: 13"@en , "An Island. Example: Alcatraz."@en ;
      rdfs:label "Island"@en ;
      owl:subClassOf ns:Place .

ns:MusicRight
      a       owl:Class ;
      rdfs:comment "The type of right associated with the audio album/track "@en ;
      rdfs:label "MusicRight"@en ;
      owl:subClassOf ns:DistributionRight .

ns:kioskAt
      a       :FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "kioskAt connects a kiosk (ATM or Redbox for example) to the local business it is at (a Safeway of Walgreens for example)"@en ;
      rdfs:domain ns:Kiosk ;
      rdfs:label "kioskAt"@en ;
      rdfs:range ns:LocalBusiness .

ns:starAnalyst
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "starAnalyst indicates if an analyst is a star analyst or not"@en ;
      rdfs:domain ns:Analyst ;
      rdfs:label "starAnalyst"@en ;
      rdfs:range xsd:boolean .

ns:albumTrack
      a       owl:ObjectProperty ;
      rdfs:comment "The albumTrack property connects an album with the tracks that make up its constituent parts."@en ;
      rdfs:domain ns:Album ;
      rdfs:label "albumTrack"@en ;
      rdfs:range
              [ a       owl:Class ;
                :unionOf (ns:AlbumTrack ns:Song)
              ] .

ns:recurrenceRule
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Conforms to the iCalendar standard: http://www.ietf.org/rfc/rfc2445.txt (page 116)"@en , "Indicates how often and when an event recurs as well as the recurrence end date if present, e.g., monthly, until January 2015."@en ;
      rdfs:domain ns:Event ;
      rdfs:label "recurrenceRule"@en ;
      rdfs:range ns:rRule .

ns:Producer
      a       owl:Class ;
      rdfs:comment "A producer is an individual responsible for the production of a film, television program, or musical album, composition, or song. This includes the executive producer credit."@en ;
      rdfs:label "Producer"@en ;
      owl:subClassOf ns:Person .

ns:primarySIC
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "This field contains the 6-digit standard industry classification code for the business's primary activity. IinfoUSA source data."@en ;
      rdfs:domain ns:SingleLocationBusiness ;
      rdfs:label "primarySIC"@en ;
      rdfs:range owl:Literal .

ns:areaCode
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Area code of a phone number"@en ;
      rdfs:domain ns:Phone ;
      rdfs:label "areaCode"@en ;
      rdfs:range xsd:int ;
      owl:subPropertyOf :topDataProperty .

ns:gameVenueOf
      a       :FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "Connects a Game to the Venue it was played in."@en ;
      rdfs:domain ns:SportVenue ;
      rdfs:label "gameVenueOf"@en ;
      rdfs:range
              [ a       owl:Class ;
                :unionOf (ns:SportCompetitionInstance ns:SportGame)
              ] ;
      :inverseOf ns:gameVenue .

ns:memberOf
      a       owl:ObjectProperty ;
      rdfs:comment "The memberOf relation connects a group to the people or subgroups that comprise its membership."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:Group ns:Person)
              ] ;
      rdfs:label "memberOf"@en ;
      rdfs:range ns:Group .

ns:Coach
      a       owl:Class ;
      rdfs:comment "A person who coaches either an individual athlete or a sports team."@en ;
      rdfs:label "Coach"@en ;
      owl:subClassOf ns:Person .

ns:finaleDate
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "original date the finale of a TV show aired. ISO 8601 extended."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:TvSeason ns:TvSeries)
              ] ;
      rdfs:label "finaleDate"@en ;
      rdfs:range
              [ a       owl:Datatype ;
                :unionOf (xsd:date xsd:gYearMonth)
              ] ;
      owl:subPropertyOf ns:date .

ns:performerName
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Name by which a performer is known."@en ;
      rdfs:domain ns:Performer ;
      rdfs:label "performerName"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf ns:name .

ns:newCar
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Indicates if the make is currently in production (likely definition). This information is derived from model, trim details in the enrichment phase "@en ;
      rdfs:domain ns:AutoMake ;
      rdfs:label "newCar"@en ;
      rdfs:range xsd:boolean .

ns:county
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "The county of a physical location"@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:Address ns:SingleLocationBusiness)
              ] ;
      rdfs:label "county"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf :topDataProperty .

ns:uuid
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Unique identifier for an object."@en ;
      rdfs:domain ns:Entity ;
      rdfs:label "uuid"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf ns:id .

ns:quotableInstrumentOf
      a       :FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "quoteableInstrumentOf connects a quoteable instruemnt to its financial instrument"@en ;
      rdfs:domain ns:QuotableInstrument ;
      rdfs:label "quoteableInstrumentOf"@en ;
      rdfs:range ns:FinancialInstrument ;
      :inverseOf ns:quotableInstrument .

ns:Organization
      a       owl:Class ;
      rdfs:comment " An organization is an organized body of members or people with a particular purpose."@en ;
      rdfs:label "Organization"@en ;
      owl:subClassOf ns:Entity .

ns:statusChangeType
      a       owl:DatatypeProperty ;
      rdfs:comment "statusChangeType captures the type of coroporate status change"@en ;
      rdfs:domain ns:OrganizationStatusChange ;
      rdfs:label "statusChangeType"@en ;
      rdfs:range owl:Literal .

ns:soundSystem
      a       owl:DatatypeProperty ;
      rdfs:comment "Indicates the type of sound system available at a venue."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:MovieShowtime ns:MovieTheater)
              ] ;
      rdfs:label "soundSystem"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf :topDataProperty .

ns:contestantOn
      a       owl:ObjectProperty ;
      rdfs:comment "Connects a game show to a contestant who appeared on it."@en ;
      rdfs:domain ns:Contestant ;
      rdfs:label "contestantOn" ;
      rdfs:range
              [ a       owl:Class ;
                :unionOf (ns:TvEpisode ns:TvSeason ns:TvSeries ns:TvSpecial)
              ] ;
      owl:subPropertyOf :topObjectProperty ;
      :inverseOf ns:contestant .

[]    a       :AllDisjointClasses ;
      :members (ns:AthleticConference ns:SportDivision ns:SportLeague ns:SportTeam) .

ns:familyName
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Note for Media V2 schema: Change to familyName to support Trailblazers globalization guidelines: http://twiki.corp.yahoo.com/view/TrailBlazers/PersonalNames"@en , "A family name is a surname added to a given name and is part of a personal name."@en ;
      rdfs:domain ns:Person ;
      rdfs:label "familyName"@en ;
      rdfs:range owl:Literal ;
      owl:subPropertyOf ns:name .

ns:astrologicalSign
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Designates the astrological sign of a person. "@en ;
      rdfs:domain ns:Person ;
      rdfs:label "astrologicalSign"@en ;
      rdfs:range owl:Literal .

ns:ETN
      a       owl:Class ;
      rdfs:comment "ETN is exchange traded note"@en ;
      rdfs:label "ETN"@en ;
      owl:subClassOf ns:ETO .

ns:numEmployees
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "numEmployees indicates the number of full-time employees"@en ;
      rdfs:domain ns:Company ;
      rdfs:label "numEmployees"@en ;
      rdfs:range xsd:float .

ns:accessibilityDescription
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes any special features of the wheelchair accessibilityof a business."@en ;
      rdfs:domain ns:LocalBusiness ;
      rdfs:label "accessibilityDescription"@en ;
      rdfs:range owl:Literal .

ns:Founder
      a       owl:Class ;
      rdfs:comment "Founder represents the person or persons who founded a company."@en ;
      rdfs:label "Founder"@en ;
      owl:subClassOf ns:FinancePerson .

ns:lyricsTo
      a       owl:ObjectProperty ;
      rdfs:comment "Attaches the lyrics class to its song or performance"@en ;
      rdfs:domain ns:Lyrics ;
      rdfs:label "lyricsTo"@en ;
      rdfs:range
              [ a       owl:Class ;
                :unionOf (ns:MusicPerformance ns:Song)
              ] ;
      owl:subPropertyOf :topObjectProperty ;
      :inverseOf ns:lyrics .

ns:PointOfInterest
      a       owl:Class ;
      rdfs:comment "Placetype: 20"@en , "A point of interest such as a school, hospital, tourist attraction. Example: statue of Liberty"@en ;
      rdfs:label "PointOfInterest"@en ;
      owl:subClassOf ns:Place .

ns:autoStreetPrice
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "The street price of a car is the price at which a car actually sells."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:AutoMake ns:AutoModel ns:AutoTrim ns:Automobile)
              ] ;
      rdfs:label "autoStreetPrice"@en ;
      rdfs:range xsd:double .

ns:weekendGross
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Gross box office receipts for a film in a given weekend."@en ;
      rdfs:domain ns:BoxOfficeWeekend ;
      rdfs:label "weekendGross"@en ;
      rdfs:range xsd:float ;
      owl:subPropertyOf :topDataProperty .

ns:lodgingDetailURL
      a       owl:DatatypeProperty ;
