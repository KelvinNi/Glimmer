      rdfs:domain ns:Lodging ;
      rdfs:label "lodgingDetailURL"@en ;
      rdfs:range xsd:anyURI .

ns:endYear
      a       :FunctionalProperty , owl:DatatypeProperty ;
      rdfs:comment "Year that an event ends."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:Event ns:Organization ns:TvSeason)
              ] ;
      rdfs:label "endYear"@en ;
      rdfs:range xsd:gYear ;
      owl:subPropertyOf ns:date .

ns:studentOf
      a       :FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "Connects an Education class to the EducationalInstitution where the study has been carried out."@en ;
      rdfs:domain ns:Education ;
      rdfs:label "studentOf"@en ;
      rdfs:range ns:EducationalInstitution ;
      owl:subPropertyOf :topObjectProperty .

ns:paymentType
      a       owl:DatatypeProperty ;
      rdfs:comment "Enumerates the types of payment accepted by a business."@en ;
      rdfs:domain
              [ a       owl:Class ;
                :unionOf (ns:LocalBusiness ns:LocalEvent)
              ] ;
      rdfs:label "paymentType"@en ;
      rdfs:range owl:Literal ;
      rdfs:range
              [ a       owl:Datatype ;
                :oneOf  [ a       rdf:List ;
                          rdf:first "CASH"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "CHECK"^^xsd:string ;
                                    rdf:rest
                                            [ a       rdf:List ;
                                              rdf:first "COUPON ACCOR"^^xsd:string ;
                                              rdf:rest
                                                      [ a       rdf:List ;
                                                        rdf:first "COUPON SUDEXHO"^^xsd:string ;
                                                        rdf:rest
                                                                [ a       rdf:List ;
                                                                  rdf:first "COUPON TIMES TICKET"^^xsd:string ;
                                                                  rdf:rest
                                                                          [ a       rdf:List ;
                                                                            rdf:first "CREDIT CARD"^^xsd:string ;
                                                                            rdf:rest
                                                                                    [ a       rdf:List ;
                                                                                      rdf:first "DEBIT CARD"^^xsd:string ;
                                                                                      rdf:rest
                                                                                              [ a       rdf:List ;
                                                                                                rdf:first "GOOGLE CHECKOUT"^^xsd:string ;
                                                                                                rdf:rest
                                                                                                        [ a       rdf:List ;
                                                                                                          rdf:first "PAYPAL"^^xsd:string ;
                                                                                                          rdf:rest
                                                                                                                  [ a       rdf:List ;
                                                                                                                    rdf:first "TRAVELERS CHECK"^^xsd:string ;
                                                                                                                    rdf:rest
                                                                                                                            [ a       rdf:List ;
                                                                                                                              rdf:first "WIRE TRANSFER"^^xsd:string ;
